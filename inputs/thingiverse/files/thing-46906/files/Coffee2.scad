module CONE(D,H,W){
	difference(){
		cylinder(r1=D/2+W,r2=(D/2)+W+H*0.08441,h=H,$fn=100);
		translate([0,0,-.1]) cylinder(r1=D/2,r2=(D/2)+(H+2)*0.08441,h=H+2,$fn=100);
	}
}
module Body(D,H,W){
	difference(){
		union(){
			cylinder(r1=D/2+W,r2=(D/2)+W+H*0.08441,h=H,$fn=100);
			//handle

					translate([-D/2-13,0,H/2]) scale([.75,1,1])rotate([90,0,0]){
						difference(){
							cylinder(r=H/2+2,h=8,$fn=80,center=true);
							cylinder(r=H/2-6,h=12,$fn=80,center=true);
						}
					if (SUPPORT==1){
						cylinder(r=H/2,h=.5,center=true);
					}
				}

		}
		translate([0,0,-.1]) cylinder(r1=D/2,r2=(D/2)+(H+2)*0.08441,h=H+2,$fn=100);
		if (CupMode == 1){
            #translate([0,-.5,-1])cube([100,1,H+2]);
        }
		//clean top and bottom
		translate([-100,-100,-20]) cube([200,200,20]);
		translate([-100,-100,H-.1]) cube ([200,200,20]);
	}
    if (CupMode == 0){
            cylinder(r=H/2-2,h=Wall,$fn=80,center=true);
        }
	
}

