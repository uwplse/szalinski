 */
module fanBladePartB()
{
    difference()
    {
        translate([2, -10, 0])
        square([3,5]);

        translate([3.2, -5, 0])
        circle(r=1.8, $fn=30);
    }    
}

module fanHub()
{
    difference()
    {
        circle(r=3, $fn=30);
        
        circle(r=2, $fn=30);
    }
}

