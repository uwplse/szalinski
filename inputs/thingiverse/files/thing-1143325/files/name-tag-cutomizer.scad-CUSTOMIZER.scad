module fan2D()
{
    union()
    {
        fanHub();
        
        fanBlades();
    }
}

module fanBlade()
{
    difference()
    {
        fanBladePartA();

        translate([0, 4.5, 0])
        fanBladePartB();
    }
}

module fanBlades()
{
    for(i = [0 : 3])
    {
//        echo(i);
        
        rotate([0,0,i*90])
        translate([7,0,0])
        
        fanBlade();
    }
}

/**
