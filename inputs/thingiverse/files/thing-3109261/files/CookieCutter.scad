  aend=a2>0?a2:angle;
  $fn=abs(astart-aend)/$fn;
  //echo(str("Angle: ", astart, "-", aend, " $fn=",$fn));
  wpoints=arc(astart,aend,r,$fn);
  //echo(str("arc: ", wpoints));
  polygon(concat([sp],wpoints));
}
