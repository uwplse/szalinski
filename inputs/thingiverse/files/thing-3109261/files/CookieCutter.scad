base_w=80; // [20:200]
// Base Height (Approximate, does not affect all parts)
base_h=70; // [20:200]
// Main Thickness
cuttert=3; // [0.4:0.2:8.0]
// Offset Mode
offmode=2; // [1,2]

/* [Custom Text] */
// Text
